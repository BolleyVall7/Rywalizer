{
    "compilerOptions": {
        "allowJs": true,
        "esModuleInterop": true,
        "noImplicitAny": false,
        "sourceMap": true,
        "baseUrl": ".",
        "outDir": "electron",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "jsx": "react",
        "target": "ES2020",
        "paths": {
            "@/*": [
                "src/electron/*"
            ]
        },
    }
}