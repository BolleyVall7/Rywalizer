{
    "compilerOptions": {
        "allowJs": true,
        "esModuleInterop": true,
        "noImplicitAny": false,
        "sourceMap": true,
        "baseUrl": ".",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "target": "ES2020",
        "paths": {
            "@/*": [
                "resources/assets/*"
            ]
        }
    },
    "include": ["resources/assets/**/*"],
}